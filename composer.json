{
    "name": "accounting-sh/php_sdk",
    "description": "Accounting.sh PHP SDK",
    "keywords": [
        "accounting",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "unlicense",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4.5",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "overtrue/phplint": "^9.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": { "Accounting\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Accounting\\Test\\" : "tests/" }
    },
    "scripts": {
        "test": [
            "@phplint",
            "vendor/bin/phpunit tests"
        ],
        "phplint": "phplint"
    }
}
